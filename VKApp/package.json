{
  "name": "vkapp",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:web": "expo export --platform web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "docker:dev": "docker-compose up vkapp-dev",
    "docker:prod": "docker-compose --profile production up vkapp-prod",
    "docker:build": "docker build -t vkapp .",
    "docker:build:dev": "docker build --target dev -t vkapp:dev .",
    "docker:build:prod": "docker build --target production -t vkapp:prod .",
    "precommit": "npm run lint && npm run type-check && npm run test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo/image-utils": "^0.8.7",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@stripe/stripe-react-native": "0.50.3",
    "@supabase/supabase-js": "^2.57.4",
    "expo": "~54.0.7",
    "expo-camera": "^17.0.7",
    "expo-document-picker": "^12.0.2",
    "expo-image-picker": "^17.0.8",
    "expo-location": "^19.0.7",
    "expo-notifications": "^0.32.11",
    "expo-secure-store": "^15.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.1"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.81.1",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.45.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-expo": "^49.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
